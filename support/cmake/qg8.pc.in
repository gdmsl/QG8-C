prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=@CMAKE_INSTALL_PREFIX@
libdir=${exec_prefix}/@QG8_LIB_DIR@
includedir=${prefix}/@QG8_INC_DIR@

Name: qg8
Description: C libraries for implementing, writing, and reading QG8 graphs.
Version: @QG8_VERSION@
Libs: -L${libdir} -l@QG8_LIB_NAME@
Cflags: -I${includedir}

